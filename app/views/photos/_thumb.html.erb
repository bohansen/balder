<% if photo.content_type.to_s.include?('webm') %>
<%= link_to (image_tag "album_video.png"), [photo.album.collections.first, photo.album, photo] %>
<% else %>
<%= link_to (image_tag eval('photo.file.' + (defined?(photosize) ? photosize : "album") + '.url') ), [photo.album.collections.first, photo.album, photo] %>
<% end %>
